<Application x:Class="tcpTrigger.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:tcpTrigger.Editor"
             StartupUri="MainWindow.xaml"
             ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/ButtonStyle.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/CheckBoxStyle.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/GroupBoxStyle.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/TabControlStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
